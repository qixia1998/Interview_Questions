syntax = "v1"

type (
	LoginRequest {
		Name     string `form:"name"`
		Password string `form:"password"`
	}
	LoginResponse {
		Code int64  `json:"code"`
		Msg  string `json:"msg"`
	}
)

service User {
	@handler Login // TODO: replace handler name and delete this comment
	post /api/user/login(LoginRequest) returns (LoginResponse)
}